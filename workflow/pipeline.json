{
  "name": "Weather_ETL_Pipeline",
  "email_notifications": {
    "on_failure": [
      "ziaarzoo21@gmail.com"
    ],
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "notification_settings": {
    "no_alert_for_skipped_runs": false,
    "no_alert_for_canceled_runs": false
  },
  "timeout_seconds": 0,
  "schedule": {
    "quartz_cron_expression": "14 55 23 * * ?",
    "timezone_id": "UTC",
    "pause_status": "UNPAUSED"
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "extract_raw",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Repos/ziaarzoo21@gmail.com/spark-etl/notebooks/Extract data",
        "source": "WORKSPACE"
      },
      "max_retries": 1,
      "min_retry_interval_millis": 60000,
      "retry_on_timeout": true,
      "disable_auto_optimization": false,
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Transform_Silver",
      "depends_on": [
        {
          "task_key": "extract_raw"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Repos/ziaarzoo21@gmail.com/spark-etl/notebooks/Transform Data",
        "source": "WORKSPACE"
      },
      "max_retries": 1,
      "min_retry_interval_millis": 60000,
      "retry_on_timeout": true,
      "disable_auto_optimization": false,
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Load_Gold",
      "depends_on": [
        {
          "task_key": "Transform_Silver"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Repos/ziaarzoo21@gmail.com/spark-etl/notebooks/load Data",
        "source": "WORKSPACE"
      },
      "max_retries": 1,
      "min_retry_interval_millis": 60000,
      "retry_on_timeout": true,
      "disable_auto_optimization": false,
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    }
  ],
  "queue": {
    "enabled": true
  },
  "performance_target": "PERFORMANCE_OPTIMIZED",
  "run_as": {
    "user_name": "ziaarzoo21@gmail.com"
  }
}